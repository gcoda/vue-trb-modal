{
  "name": "vue-trb-modal",
  "version": "1.0.7",
  "main": "dist/vue-trb-modal.umd.js",
  "module": "dist/vue-trb-modal.esm.js",
  "unpkg": "dist/vue-trb-modal.min.js",
  "browser": {
    "./sfc": "src/components/Modal.vue"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "demo:build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-service": "^3.1.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.4",
    "cross-env": "^5.2.0",
    "minimist": "^1.2.0",
    "rollup": "^1.4.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.7.2",
    "vue-template-compiler": "^2.6.8",
    "core-js": "^2.6.5",
    "vue": "^2.6.6"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
